<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="profileOneVM" type="com.application.app.modules.profileone.data.viewmodel.ProfileOneVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnbg"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearProfileOne"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStackbg"
                android:layout_height="@dimen/_518pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageBg"
                    android:layout_gravity="start"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_152pxh}"
                    imageRes="@{@drawable/img_bg_teal_50}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_bg_teal_50"/>
                <LinearLayout android:id="@+id/linearColumnarrowleft"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginEnd="@dimen/_13pxh"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_19pxh"
                        android:layout_marginEnd="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_width="@dimen/_19pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <FrameLayout
                        android:id="@+id/frameStackrectangleseven"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_136pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_21pxv" android:layout_width="@dimen/_136pxh">
                        <ImageView android:id="@+id/imageRectangleSeven"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_136pxh"
                            android:layout_width="@dimen/_136pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_rectangle7_black_900"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle7_black_900"/>
                        <ImageView android:id="@+id/imageRectangleOne"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_124pxh"
                            android:layout_marginBottom="@dimen/_6pxv"
                            android:layout_marginEnd="@dimen/_6pxh"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="@dimen/_124pxh"
                            android:scaleType="fitXY"
                            cornerRadius="@{@dimen/_35pxh}"
                            imageRes="@{@drawable/img_rectangle1}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle1"/>
                    </FrameLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtJohnDoe"
                        android:layout_gravity="end"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_8pxh"
                        android:layout_marginStart="@dimen/_115pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{profileOneVM.profileOneModel.txtJohnDoe}"
                        style="@style/txtPoppinsbold25" tools:text="@string/lbl_john_doe"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtName"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_186pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{profileOneVM.profileOneModel.txtName}"
                        style="@style/txtPoppinsbold22" tools:text="@string/lbl_name"/>
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtAndroidTutoria"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_17pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{profileOneVM.profileOneModel.txtAndroidTutoria}"
                        style="@style/txtPoppinsregular22" tools:text="@string/msg_android_tutoria"/>
                </LinearLayout>
            </FrameLayout>
            <TextView android:ellipsize="end" android:id="@+id/txtEmail"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_17pxh"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{profileOneVM.profileOneModel.txtEmail}"
                style="@style/txtPoppinsbold22" tools:text="@string/lbl_email"/>
            <TextView android:ellipsize="end" android:gravity="start"
                android:id="@+id/txtEmailOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{profileOneVM.profileOneModel.txtEmailOne}"
                style="@style/txtPoppinsregular22" tools:text="@string/msg_qwertyuio_gmail"/>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnsignout"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_51pxv"
                android:layout_width="wrap_content"
                android:orientation="vertical" style="@style/groupStyleteal_50cornerRadius">
                <TextView android:ellipsize="end"
                    android:gravity="start" android:id="@+id/txtSIGNOUT"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginEnd="@dimen/_71pxh"
                    android:layout_marginStart="@dimen/_71pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{profileOneVM.profileOneModel.txtSIGNOUT}"
                    style="@style/txtPoppinsbold22" tools:text="@string/lbl_sign_out"/>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottomappbarrectangletwelve"
            android:layout_height="wrap_content" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameStackrectangletwelve"
                android:layout_height="@dimen/_111pxv" android:layout_width="match_parent">
                <View
                    android:background="@drawable/rectangle_bg_white_a700_radius_20"
                    android:id="@+id/viewRectangleTwelve"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_82pxv"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_375pxh"/>
                <LinearLayout android:id="@+id/linearRowhome"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_45pxv"
                    android:layout_marginEnd="@dimen/_41pxh"
                    android:layout_marginStart="@dimen/_41pxh"
                    android:layout_marginTop="@dimen/_45pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageHome"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_home"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                    <ImageView android:id="@+id/imageVector"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_43pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowfavorite"
                    android:layout_gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_43pxv"
                    android:layout_marginEnd="@dimen/_47pxh"
                    android:layout_marginStart="@dimen/_47pxh"
                    android:layout_marginTop="@dimen/_43pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageFavorite"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_favorite"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_favorite"/>
                    <ImageView android:id="@+id/imageUser"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_37pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                </LinearLayout>

                <com.google.android.material.floatingactionbutton.FloatingActionButton
                    android:id="@+id/floatingBtnFloatingactionbutton"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_143pxh"
                    android:layout_marginStart="@dimen/_143pxh"
                    android:layout_width="wrap_content"
                    app:fabCustomSize="@dimen/_86pxh"
                    app:srcCompat="@drawable/img_rewind_white_a700" app:tint="@color/white_A700"/>
            </FrameLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
