<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="splashVoneVM" type="com.application.app.modules.splashvone.data.viewmodel.SplashVoneVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackcomponentone"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <FrameLayout android:id="@+id/frameSplashvOne"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStylewhite_A700">
            <FrameLayout android:id="@+id/frameButtonnext"
                android:layout_gravity="bottom|end"
                android:layout_height="@dimen/_366pxv"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_265pxh">
                <ImageView android:id="@+id/imageComponentOne"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_366pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_component1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_component1"/>
                <LinearLayout android:id="@+id/linearRownext"
                    android:layout_gravity="top|end"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_156pxv"
                    android:layout_marginEnd="@dimen/_52pxh"
                    android:layout_marginStart="@dimen/_52pxh"
                    android:layout_marginTop="@dimen/_156pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtNext"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.73"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{splashVoneVM.splashVoneModel.txtNext}"
                        style="@style/txtPoppinsregular20" tools:text="@string/lbl_next"/>
                    <ImageView android:id="@+id/imageArrowright"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowright"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
                </LinearLayout>
            </FrameLayout>
            <FrameLayout android:id="@+id/frameStackbg"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_758pxv"
                android:layout_marginBottom="@dimen/_10pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageBg"
                    android:layout_gravity="start"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_bg}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_bg"/>
                <LinearLayout android:id="@+id/linearColumnleftside"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <androidx.appcompat.widget.Toolbar
                        android:gravity="start"
                        android:id="@+id/toolbarToolbar"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" app:contentInsetStart="0dp">
                        <LinearLayout
                            android:id="@+id/linearModeLightTyp"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageLeftSide"
                                android:layout_height="@dimen/_21pxh"
                                android:layout_marginTop="@dimen/_12pxv"
                                android:layout_width="@dimen/_54pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_leftside"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_leftside"/>
                            <FrameLayout
                                android:id="@+id/frameStacknotch"
                                android:layout_height="@dimen/_28pxv"
                                android:layout_marginBottom="@dimen/_4pxv"
                                android:layout_marginStart="@dimen/_3pxh" android:layout_width="@dimen/_282pxh">
                                <ImageView android:id="@+id/imageNotch"
                                    android:layout_gravity="start"
                                    android:layout_height="@dimen/_28pxh"
                                    android:layout_marginBottom="@dimen/_1pxv"
                                    android:layout_marginEnd="@dimen/_10pxh"
                                    android:layout_width="@dimen/_219pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_notch"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_notch"/>
                                <ImageView
                                    android:id="@+id/imageRightSide"
                                    android:layout_gravity="bottom|end"
                                    android:layout_height="@dimen/_11pxh"
                                    android:layout_marginStart="@dimen/_10pxh"
                                    android:layout_marginTop="@dimen/_10pxv"
                                    android:layout_width="@dimen/_66pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_rightside"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_rightside"/>
                            </FrameLayout>
                        </LinearLayout>
                    </androidx.appcompat.widget.Toolbar>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtWelcometo"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_87pxh"
                        android:layout_marginStart="@dimen/_87pxh"
                        android:layout_marginTop="@dimen/_85pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{splashVoneVM.splashVoneModel.txtWelcometo}"
                        style="@style/txtPoppinsregular16" tools:text="@string/lbl_welcome_to"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSocially"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_87pxh"
                        android:layout_marginStart="@dimen/_87pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{splashVoneVM.splashVoneModel.txtSocially}"
                        style="@style/txtPoppinsbold39" tools:text="@string/lbl_socially"/>
                    <ImageView android:gravity="center_horizontal"
                        android:id="@+id/imageHeroimg"
                        android:layout_height="@dimen/_256pxh"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_9pxh"
                        android:layout_marginTop="@dimen/_62pxv"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_heroimg"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_heroimg"/>

                    <com.asksira.loopingviewpager.indicator.CustomShapePagerIndicator
                        android:id="@+id/indicatorMusic"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_87pxh"
                        android:layout_marginStart="@dimen/_87pxh"
                        android:layout_marginTop="@dimen/_51pxv"
                        android:layout_width="wrap_content"
                        app:drawable_selected="@drawable/indicator_selected_bg_black_900_radius_8"
                        app:indicator_size="@dimen/_11pxh" app:indicator_spacing="@dimen/_7pxh"/>
                </LinearLayout>
            </FrameLayout>
        </FrameLayout>
    </FrameLayout>
</layout>
